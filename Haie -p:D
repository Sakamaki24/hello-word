 +    Paste_From_Word:  'Wordden yaptr',
 +    Tools:            'Aralar',
 +    Containers:       'Kapsayclar',
 +    Classes:          'Snflar',
 +    Status:           'Durum',
 +    Source_Code:      'Kaynak Kodu'
 +};
 +
