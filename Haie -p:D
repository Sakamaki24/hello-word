View 18  media/static/zinnia/sass/mixins/_delimited-list.scss
@@ -0,0 +1,18 @@
 +@mixin delimited-list($separator: ", ") {
 +  li {
 +    list-style: none;
 +    display: inline;
 +    &:after {
 +      content: $separator;
 +    }
 +    &:last-child {
 +      &:after {
 +        content: "";
 +      }
 +    }
 +    &.last {
 +      &:after {
 +        content: ""; }
 +    }
 +  }
 +}
