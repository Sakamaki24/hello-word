 +        WYMeditor.XhtmlSaxListener = function () {
 +            var listener = XhtmlSaxListener.call(this);
 +            listener.block_tags.push('embed');
 +            return listener;
 +        };
 +        WYMeditor.XhtmlSaxListener.prototype = XhtmlSaxListener.prototype;
 +    }
 +})();
